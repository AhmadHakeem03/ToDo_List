<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>To Do List</title>
    <link rel="stylesheet" href="main.css" />
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;700&display=swap"
    />
  </head>
  <body>
    <div class="container">
      <div class="nav">
        <ul>
            <a href="/school"><li><img src="data:image/png;base64," class = "school" />school</li></a>
            <a href="/today"><li><img src="data:image/png;base64," class ="school"/> Today</li></a>
        </ul>
      </div>
      <div class="list0">
        <div class="list">
            <div class="date">
                <h1>HomeWorks for <%= new Date().getDate() %>th <%= new Date().getMonth()+1  %> </h1>
            </div>
            <form  class="input" action = "\school" method="Post">
                <input type="text" class = "txt" name = "fName">
                <button type = "submit" name = "" class = "adding">+</button>
            </form>
        <hr>
        <div class="AddingItems">
            <% if(locals.homeworks){ %>
                <% homeworks.forEach(homework => { %>
                    <div class="item">
                    <li><%= homework %></li>
                    <input type="checkbox" class="ui-checkbox">
                </div>
                  <% }); %>
                <% }  %>
        </div>
        </div>
      </div>
      <div class="animi"></div>
    </div>
  </body>
</html>
